@model IEnumerable<NguyenMinhPhuong__2210900141_k22cnt4_Project2.Models.Role>

@{
    ViewBag.Title = "Quản lý quyền truy cập";
    Layout = "~/Areas/NMPAdmin/Views/Shared/_NmpAdminLayout.cshtml";
}
<div class="page-header">
    <div class="header-sub-title">
        <nav class="breadcrumb breadcrumb-dash">
            <a href="#" class="breadcrumb-item"><i class="anticon anticon-home m-r-5"></i>Home</a>
            <a class="breadcrumb-item" href="/NMPAdmin/NmpRoles/Index">Danh sách quyền truy cập</a>
           
        </nav>
    </div>
</div>
<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Mô tả</th>
                @*<th scope="col">#</th>*@
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {

                    <tr>
                        <th scope="row">@item.RoleID</th>
                        <td>@item.RoleName</td>
                        <td>@item.Description</td>
                        <td>
                        <td>
                            <a class="btn btn-success btn-tone m-r-5" @Html.ActionLink("Create", "Create", new { id = item.RoleID })></a>
                            <a class="btn btn-primary btn-tone m-r-5" @Html.ActionLink("Edit", "Edit", new { id = item.RoleID })></a>
                            <a class="btn btn-secondary btn-tone m-r-5" @Html.ActionLink("Details", "Details", new { id = item.RoleID })></a>
                            <a class="btn btn-success btn-tone m-r-5" @Html.ActionLink("Delete", "Delete", new { id = item.RoleID })></a>
                        </td>
                       

                        
                    </tr>

                }
            }
        </tbody>
    </table>
</div>

